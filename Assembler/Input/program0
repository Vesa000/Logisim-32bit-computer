	LDV	0	0		//Load 0 to reg 0
	LDV	1	1		//Load 1 to reg 1
	LDV	100	2		//Load 100 to reg 2
add1:
	ADD	0	1	0	//reg 0 + reg 1 to reg 0
	CMPU	0	2		//Compare reg0 to reg2
LT	JMP	add1			//if less than jump to add1

	BRA	dosomething		//Branch to dosomething;

	ADD	0	3	4	//Reg0 + Reg3 to Reg4
	HLT				//Halt

dosomething:
	LDV	100	3		//Load 100 to reg3
	RET				//Return to Branch start point